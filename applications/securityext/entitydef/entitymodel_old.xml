<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
	<title>Entity of an Apache OFBiz Component</title>
	<description>None</description>
	<version>1.0</version>

	<entity entity-name="App" table-name="APP" package-name="org.ofbiz.securityext" title="App oauth">
		<field name="appKey" type="id-ne" />
		<field name="secretKey" type="id-ne" />
		<field name="callbackUrl" type="long-varchar" />
		<field name="description" type="description" />
		<field name="appName" type="id" />
		<field name="appStatusId" type="id" />
		<field name="create_time" type="date-time" />
		<field name="appOwner" type="id-ne" />
		<field name="approval" type="id" />
		<prim-key field="appKey" />
	</entity>

	<entity entity-name="Authority" table-name="AUTHORITY" package-name="org.ofbiz.securityext" title="App oauth">
		<field name="appKey" type="id-ne" />
		<field name="userLoginId" type="id-ne" />
		<prim-key field="appKey" />
		<prim-key field="userLoginId" />
	</entity>
	
	<entity entity-name="AppApiAuth" table-name="APP_API_AUTH" package-name="org.ofbiz.securityext" title="Api oauth">
		<field name="appid" type="id-ne" />
		<field name="appsecret" type="id-ne" />
		<field name="appPublicKey" type="id-ne" />
		<field name="appPrivateKey" type="id-ne" />
		<field name="signType" type="id-ne" />
		<prim-key field="appid" />
	</entity>
	
	<entity entity-name="RefreshToken" table-name="REFRESH_TOKEN" package-name="org.ofbiz.securityext" title="App oauth">
		<field name="appKey" type="id-ne" />
		<field name="userLoginId" type="id-ne" />
		<field name="grantType" type="id-ne" />
		<field name="accessToken" type="id-ne" />
		<field name="refreshToken" type="id-ne" />
		<field name="createTime" type="date-time" />
		<field name="authorizationTime" type="date-time" />
		<field name="expire" type="numeric" />
		<field name="scope" type="long-varchar" />
		<prim-key field="appKey" />
		<prim-key field="userLoginId" />
	</entity>

</entitymodel>